{
  "configurations": [
    {
      "name": "windows-gcc-x64",
      "includePath": [
        "${workspaceFolder}/**",
        "${env:MINGW64}/x86_64-w64-mingw32/include/**",
        "${env:RAYLIB_5}/include/**"
      ],
      "defines": [
        "_DEBUG",
        "UNICODE",
        "_UNICODE",
        "GRAPHICS_API_OPENGL_33",
